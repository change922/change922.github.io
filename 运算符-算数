public class 算数运算符 {
	public static void main(String[] args) {
		byte a = 3;
		int b = 4;
		long f = 5l;
		int c = a+b;
		long h = f+c;//计算的数有一个为long类型则结果为long类型。
		System.out.println(b+f);
		System.out.println(h);
		//浮点数可以是任意整数计算的结果类型，因为浮点的范围比整型的范围大。
		double a1 = 2.11;
		float c1 = 3.14f;
		double g = a1+b;//有一个为double则结果为double。两边都为float的浮点计算结果才是float。
		float d=a+c;
		System.out.println(d);
		float j = h+c;
		System.out.print(j);/*+“算术运算符的顺序为，按照参与计算的数据类型的范围而定，最后
		的结果为范围最大的”）;*/
}
}
